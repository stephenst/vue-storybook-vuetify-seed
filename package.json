{
  "name": "vue2book",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test --coverage coverageDirectory=coverage/jest",
    "lint": "vue-cli-service lint",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 6006",
    "test:unit": "vue-cli-service test:unit",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "d3": "^5.7.0",
    "d3-delaunay": "^4.1.5",
    "d3-sankey": "^0.12.3",
    "leaflet": "^1.4.0",
    "leaflet-geometryutil": "^0.9.1",
    "leaflet.heat": "^0.2.0",
    "leaflet.pattern": "^0.1.0",
    "milsymbol": "^1.3.3",
    "moment": "^2.24.0",
    "venn.js": "^0.2.20",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vue2-leaflet": "^2.1.1",
    "vuetify": "^2.0.18",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/vue": "^5.2.1",
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-plugin-unit-jest": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "coveralls": "^3.0.7",
    "deepmerge": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^4.0.1",
    "jest": "^24.9.0",
    "material-design-icons-iconfont": "^5.0.1",
    "sass": "^1.22.12",
    "sass-loader": "^7.3.1",
    "storybook-vue-router": "^1.0.7",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
